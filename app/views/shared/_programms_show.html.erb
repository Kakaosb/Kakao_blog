<p id="notice"><%= notice %></p>

<h1>Listing Blog Programms</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Tags</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blog_programms.each do |blog_programm| %>
      <tr>
        <td><%= blog_programm.title %></td>
        <td><%= blog_programm.content %></td>
        <td><%= blog_programm.tags %></td>
        <td><%= link_to 'Show', blog_programm %></td>
        <td><%= link_to 'Edit', edit_blog_programm_path(blog_programm) %></td>
        <td><%= link_to 'Destroy', blog_programm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Programm', new_blog_programm_path %>
